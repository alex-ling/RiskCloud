<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleBar">
        <attr name="leftBackground" format="reference" />
        <attr name="rightBackground" format="reference" />
        <attr name="titleText" format="string" />
        <attr name="titleTextSize" format="dimension" />
        <attr name="titleTextColor" format="color" />
    </declare-styleable>
    <declare-styleable name="CheckBox">
        <attr name="checkWidth" format="dimension" />
        <attr name="checkHeight" format="dimension" />
        <attr name="checkImage" format="reference" />
        <attr name="uncheckImage" format="reference" />
        <attr name="text" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="checked" format="boolean" />
    </declare-styleable>
</resources>